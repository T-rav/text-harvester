    datasource db {
      provider = "postgresql"
      url      = env("DATABASE_URL")
    }

    generator client {
      provider = "prisma-client-js"
    }

    model User {
      id        Int      @id @default(autoincrement())
      qid       String   @unique @default(uuid())
      firstName String
      lastName  String
      email     String   @unique
      picture   String
      cts       DateTime @default(now())
      ets       DateTime @updatedAt

      @@index([qid])
    }